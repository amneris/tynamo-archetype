<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd" xmlns:p="tapestry:parameter">

	<t:block id="check">
		<t:if test="check">
			<input type="checkbox" checked="checked" disabled="disabled"></input>
			<p:else>
				<input type="checkbox" disabled="disabled"></input>
			</p:else>
		</t:if>
	</t:block>

</t:container>
